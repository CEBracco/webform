  },
  "author": "cbracco",
  "license": "ISC",
  "dependencies": {
    "bestzip": "^2.1.7",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "dotenv-cli": "^2.0.1",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "express-force-https": "^1.0.0",
    "fs-extra": "^8.1.0",
    "hashids": "^1.2.2",
    "log4js": "^5.0.0",
    "mercadopago": "^1.3.0",
    "module-alias": "^2.2.1",
    "mysql2": "^1.6.5",
    "node-cache": "^5.1.2",
    "sequelize": "^5.15.2",
    "sequelize-cli": "^5.5.0",
    "unirest": "^0.6.0",
    "url-join": "^4.0.1"
  },
  "_moduleAliases": {
    "@appSrc": "./app/src",
