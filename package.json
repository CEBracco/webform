{
  "name": "webform",
  "version": "1.0.0",
  "description": "a simple product selection form",
  "main": "index.js",
  "scripts": {
    "start": "node --require dotenv/config index.js",
    "start:sclustered": "node --require dotenv/config indexClustered.js",
    "test": "nodemon --require dotenv/config index.js",
    "db:create": "npx dotenv sequelize db:create",
